<% if(baby.events.length > 0) { %>
	<div class="baby-container">
		<h3><%= baby.name %></h3>
		<ul class="nav nav-tabs nav-justified">
			<li role="presentation"><a href="#todo" aria-controls="todo" role="tab" data-toggle="tab">Todo</a></li>
			<li role="presentation"><a href="#comida" aria-controls="comida" role="tab" data-toggle="tab">Comida</a></li>
			<li role="presentation"><a href="#panales" aria-controls="panales" role="tab" data-toggle="tab">Pa√±ales</a></li>
			<li role="presentation"><a href="#otros" aria-controls="otros" role="tab" data-toggle="tab">Otros</a></li>
		</ul>
		<div class="tab-content">
			<%- include('babyEventsTab',{type: 'todo', withType: true, eventList: baby.events.sort(function(a,b){return b.date - a.date;})}); -%>
			<%- include('babyEventsTab',{type: 'comida', withType: true, eventList: baby.events.filter(function(evt){return (evt.eventType === 'food')}).sort(function(a,b){return b.date - a.date;})}); -%>
			<%- include('babyEventsTab',{type: 'panales', withType: true, eventList: baby.events.filter(function(evt){return (evt.eventType === 'diaper')}).sort(function(a,b){return b.date - a.date;})}); -%>
			<%- include('babyEventsTab',{type: 'otros', withType: true, eventList: baby.events.filter(function(evt){return (evt.eventType === 'others')}).sort(function(a,b){return b.date - a.date;})}); -%>
		</div>
	</div>
<% } %>